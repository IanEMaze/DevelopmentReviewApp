{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier", "@typescript-eslint", "import"],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn",
    "import/prefer-default-export": "off"
  }
}
